<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="testDAO">
	<select id="getUserVO" parameterType="egovframework.egovtest.vo.UserVO" resultType="egovframework.egovtest.vo.UserVO">
		SELECT
			USER_ID AS id, USER_PW AS pw,
			USER_NM AS name
		FROM TEST_USER
		WHERE USER_ID = #{id}
			AND USER_PW = #{pw}
	</select>
</mapper>